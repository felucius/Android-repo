<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="maximedelange.csi_week_1_rebuild.Report"
    tools:showIn="@layout/activity_report"
    style="@style/outerBackgroundStyle">

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:weightSum="1"
        style="@style/backgroundStyle">

        <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/Picture"
            android:id="@+id/button2"
            style="@style/buttonStyle.2"
            android:background="@drawable/buttonshape"/>

        <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/GPS"
            android:id="@+id/button3"
            style="@style/buttonStyle.3"
            android:background="@drawable/buttonshape"/>

        <ImageView
            android:layout_width="225dp"
            android:layout_height="200dp"
            android:id="@+id/imageView2"
            android:src="@mipmap/maxime"
            style="@style/imageviewStyle" />

        <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/Call"
            android:id="@+id/button4"
            style="@style/buttonStyle.6"
            android:background="@drawable/buttonshape"/>

        <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/Back"
            android:id="@+id/Back"
            style="@style/buttonStyle.4"
            android:background="@drawable/buttonshape"/>

        <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/Send"
            android:id="@+id/button6"
            style="@style/buttonStyle.5"
            android:background="@drawable/buttonshape"/>
    </LinearLayout>

</RelativeLayout>
